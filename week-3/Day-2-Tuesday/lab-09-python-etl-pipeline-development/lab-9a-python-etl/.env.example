# SQL Server Configuration for Python ETL Lab
# Copy this file to .env and update with your actual values

# Database connection settings for SQL Server
DB_SERVER=localhost
DB_PORT=1433
DB_NAME=python_etl_lab
DB_AUTH_TYPE=sql
DB_USER=sa
DB_PASSWORD=type-your-database-password-here
DB_DRIVER=ODBC Driver 17 for SQL Server

